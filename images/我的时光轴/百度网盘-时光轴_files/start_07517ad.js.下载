define("file-widget-1:officeEdit/start.js",function(e,i,t){var o=e("file-widget-1:officeEdit/context.js"),n={_editFileCache:{},_cacheEditFile:function(e){return e?void(n._editFileCache[e.server_filename]=e.path):n._editFileCache},openOfficeFile:function(e,i){if("doc"!==e&&"docx"!==e&&"xls"!==e&&"xlsx"!==e&&"ppt"!==e&&"pptx"!==e)throw new Error("文件格式不正确！");if(i.path){var t=i.path,o=t.replace(t.substring(t.indexOf(".")+1),e);n._cacheEditFile(i),window.open("//pan.baidu.com/disk/officedit?path="+encodeURIComponent(encodeURIComponent(o))+"&size="+i.size)}}};t.exports={start:function(e,i){var t,f,s;if(o.setContext(e),f="object"==typeof i&&i.filesList?i.filesList.length>0?i.filesList:[i.filesList]:e.list.getSelected(),1!==f.length)throw new Error("[Plugin][file-widget-1/third/officeEdit] The filesList must have one file !");t=f[0],s=o.getContext().tools.baseService.getFileCategory(t.path),n.openOfficeFile(s,t)}}});